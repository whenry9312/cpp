#include <iostream>
#include <cmath>
using namespace std;
main(){
	int a,b,c;
	int d,e,f;
	cin>>a>>b>>c;
	d=a;
	e=b;
	f=c;
	a=pow(a,2);
	b=pow(b,2);
	c=pow(c,2);

	if (a+b==c and c-b==a and c-a==b or b+c==a and a-b==c and a-c==b or a+c==b and b-a==c and b-c==a){
		cout<<"啊可以";
	}
	else{
		cout<<"啊不可以";
	}
}
